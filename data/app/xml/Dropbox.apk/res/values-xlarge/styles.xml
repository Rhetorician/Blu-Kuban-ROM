<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="SingleCharacterPasscodeBox">
        <item name="android:textAppearance">?android:textAppearanceLarge</item>
        <item name="android:textSize">38.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:gravity">center</item>
        <item name="android:singleLine">true</item>
        <item name="android:maxLength">1</item>
        <item name="android:digits">0123456789</item>
    </style>
    <style name="GalleryButton">
        <item name="android:background">@null</item>
        <item name="android:padding">16.0dip</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_weight">0.0</item>
    </style>
    <style name="LoginInput">
        <item name="android:layout_gravity">center_vertical</item>
    </style>
    <style name="BottomBarContainer">
        <item name="android:layout_gravity">center_horizontal</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minWidth">650.0dip</item>
    </style>
</resources>
