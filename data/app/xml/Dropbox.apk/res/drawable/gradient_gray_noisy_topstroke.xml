<?xml version="1.0" encoding="utf-8"?>
<layer-list
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#ff000000" />
        </shape>
    </item>
    <item android:top="1.0px">
        <shape android:shape="rectangle">
            <gradient android:startColor="#ff000000" android:endColor="#ff1a1a1a" android:angle="270.0" />
        </shape>
    </item>
    <item android:top="1.0px">
        <bitmap android:src="@drawable/noise" android:antialias="false" android:filter="false" android:tileMode="repeat" />
    </item>
</layer-list>
