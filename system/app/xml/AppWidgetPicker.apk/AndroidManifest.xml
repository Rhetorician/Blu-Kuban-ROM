<?xml version="1.0" encoding="utf-8"?>
<manifest android:versionCode="8" android:versionName="1.2.3" package="com.boombuler.system.appwidgetpicker"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <uses-permission android:name="android.permission.BIND_APPWIDGET" />
    <application android:label="@string/app_name" android:icon="@drawable/icon">
        <activity android:theme="@android:style/Theme.Dialog" android:label="AppWidgetPicker" android:name="AppWidgetPickerActivity">
            <intent-filter android:priority="999">
                <action android:name="android.appwidget.action.APPWIDGET_PICK" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
    </application>
    <uses-sdk android:minSdkVersion="7" />
</manifest>
