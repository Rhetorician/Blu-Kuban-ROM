<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="MediaButton">
        <item name="android:background">@null</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="MediaButton.Previous" parent="@style/MediaButton">
        <item name="android:src">@drawable/btn_playback_rew_holo</item>
    </style>
    <style name="MediaButton.Next" parent="@style/MediaButton">
        <item name="android:src">@drawable/btn_playback_ff_holo</item>
    </style>
    <style name="MediaButton.Play" parent="@style/MediaButton">
        <item name="android:src">@drawable/btn_playback_play_holo</item>
    </style>
    <style name="MediaButton.Pause" parent="@style/MediaButton">
        <item name="android:src">@drawable/btn_playback_pause_holo</item>
    </style>
    <style name="MediaButton.PreviousHolo" parent="@style/MediaButton">
        <item name="android:background">@null</item>
        <item name="android:src">@drawable/btn_playback_rew_holo</item>
    </style>
    <style name="MediaButton.NextHolo" parent="@style/MediaButton">
        <item name="android:background">@null</item>
        <item name="android:src">@drawable/btn_playback_ff_holo</item>
    </style>
    <style name="MediaButton.PlayHolo" parent="@style/MediaButton">
        <item name="android:background">@null</item>
        <item name="android:src">@drawable/btn_playback_play_holo</item>
    </style>
    <style name="MediaButton.PauseHolo" parent="@style/MediaButton">
        <item name="android:background">@null</item>
        <item name="android:src">@drawable/btn_playback_pause_holo</item>
    </style>
    <style name="RootTheme" parent="@android:style/Theme" />
    <style name="NoTitleBarNoShadowTheme" parent="@android:style/Theme.NoTitleBar">
        <item name="android:windowContentOverlay">@null</item>
    </style>
    <style name="DialogTheme" parent="@android:style/Theme.Dialog" />
    <style name="DialogTheme.NoActionBar" parent="@style/DialogTheme" />
    <style name="TextShadow">
        <item name="android:shadowColor">#bf000000</item>
        <item name="android:shadowRadius">2.0</item>
    </style>
    <style name="MusicMenuView">
        <item name="rowHeight">66.0dip</item>
        <item name="maxItemsPerRow">3</item>
        <item name="maxRows">2</item>
    </style>
    <style name="PopupMenuAnimation">
        <item name="android:windowEnterAnimation">@anim/popupmenu_enter</item>
        <item name="android:windowExitAnimation">@anim/popupmenu_exit</item>
    </style>
    <style name="OptionsMenuAnimation">
        <item name="android:windowEnterAnimation">@anim/options_panel_enter</item>
        <item name="android:windowExitAnimation">@anim/options_panel_exit</item>
    </style>
    <style name="MediaButton.PlayPauseButtonNormal" parent="@style/MediaButton">
        <item name="pauseImage">@drawable/btn_playback_pause</item>
        <item name="playImage">@drawable/btn_playback_play</item>
        <item name="stopImage">@drawable/btn_playback_stop</item>
    </style>
    <style name="MediaButton.PlayPauseButtonNormalHolo" parent="@style/MediaButton">
        <item name="android:background">@null</item>
        <item name="pauseImage">@drawable/btn_playback_pause_holo</item>
        <item name="playImage">@drawable/btn_playback_play_holo</item>
        <item name="stopImage">@drawable/btn_playback_stop_holo</item>
    </style>
    <style name="MediaButton.PlayPauseButtonSmall" parent="@style/MediaButton">
        <item name="pauseImage">@drawable/btn_playback_ic_pause_small</item>
        <item name="playImage">@drawable/btn_playback_ic_play_small</item>
        <item name="stopImage">@drawable/btn_playback_ic_stop_small</item>
    </style>
    <style name="AnimationDropDownDown">
        <item name="android:windowEnterAnimation">@anim/grow_fade_in</item>
        <item name="android:windowExitAnimation">@anim/shrink_fade_out</item>
    </style>
    <style name="AnimationDropDownUp">
        <item name="android:windowEnterAnimation">@anim/grow_fade_in_from_bottom</item>
        <item name="android:windowExitAnimation">@anim/shrink_fade_out_from_bottom</item>
    </style>
    <style name="TextAppearanceExtraSmall" parent="@android:style/TextAppearance.Small">
        <item name="android:textSize">12.0sp</item>
    </style>
    <style name="TextAppearanceExtraExtraSmall" parent="@android:style/TextAppearance.Small">
        <item name="android:textSize">10.0sp</item>
    </style>
    <style name="TextAppearance.SideBarTv" parent="@android:style/TextAppearance.Medium">
        <item name="android:textSize">17.0sp</item>
    </style>
    <style name="Widget.SideBarTv_ButtonText" parent="@android:style/Widget">
        <item name="android:textAppearance">@style/TextAppearance.SideBarTv</item>
        <item name="android:textColor">@color/sidebar_tv_text</item>
    </style>
    <style name="AudioFileInfoOverlayText">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:paddingLeft">4.0px</item>
        <item name="android:paddingBottom">4.0px</item>
        <item name="android:focusable">true</item>
        <item name="android:shadowColor">#ff000000</item>
        <item name="android:shadowDx">1.0</item>
        <item name="android:shadowDy">1.0</item>
        <item name="android:shadowRadius">1.0</item>
    </style>
    <style name="ToolbarBackground">
        <item name="android:background">#ff222233</item>
    </style>
    <style name="VerticalDividerLeft">
        <item name="android:background">#ff666677</item>
        <item name="android:layout_width">1.0px</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_marginLeft">4.0px</item>
    </style>
    <style name="VerticalDividerRight">
        <item name="android:background">#ff000022</item>
        <item name="android:layout_width">1.0px</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_marginRight">4.0px</item>
    </style>
    <style name="HorizontalDividerTop">
        <item name="android:background">#ff666677</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">1.0px</item>
    </style>
    <style name="HorizontalDividerBottom">
        <item name="android:background">#ff000022</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">1.0px</item>
    </style>
    <style name="SetupDivider">
        <item name="android:background">@color/divider_color</item>
        <item name="android:focusable">false</item>
        <item name="android:clickable">false</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">3.0dip</item>
        <item name="android:layout_marginTop">6.0dip</item>
        <item name="android:layout_marginBottom">7.0dip</item>
    </style>
</resources>
