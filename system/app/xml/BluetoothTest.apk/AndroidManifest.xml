<?xml version="1.0" encoding="utf-8"?>
<manifest android:sharedUserId="android.uid.system" android:versionCode="1" android:versionName="1.0" package="com.sec.android.app.bluetoothtest"
  xmlns:android="http://schemas.android.com/apk/res/android">
    <application android:label="@string/app_name" android:icon="@drawable/icon">
        <activity android:label="@string/app_name_addr" android:name=".BluetoothAddrViewer">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:label="@string/app_name_rftest" android:name=".BluetoothRfTest">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <receiver android:name=".BluetoothTestBroadcastReceiver">
            <intent-filter>
                <action android:name="android.provider.Telephony.SECRET_CODE" />
                <data android:scheme="android_secret_code" android:host="232337" />
                <data android:scheme="android_secret_code" android:host="232331" />
            </intent-filter>
        </receiver>
        <receiver android:name=".BluetoothBDAdrressReceiver">
            <intent-filter>
                <action android:name="android.intent.action.BOOT_COMPLETED" />
                <action android:name="com.sec.android.app.bluetoothtest.BT_ID_WRITE" />
                <action android:name="com.sec.android.app.bluetoothtest.BT_ID_READ" />
            </intent-filter>
        </receiver>
        <service android:name=".BluetoothBDTestService" android:process="com.android.phone">
            <intent-filter>
                <action android:name="com.bluetoothtestapp.BtBDstartservice.BootComplete" />
                <action android:name="com.sec.android.app.bluetoothtest.BT_ID_WRITE" />
                <action android:name="com.sec.android.app.bluetoothtest.BT_ID_READ" />
            </intent-filter>
        </service>
        <receiver android:name=".BluetoothTestApp" android:process="com.android.phone">
            <intent-filter>
                <action android:name="android.test.bluetooth.intent.action.HOSTREQ_BTTESTMODE" />
            </intent-filter>
        </receiver>
        <service android:name=".BluetoothTestService" android:process="com.android.phone">
            <intent-filter>
                <action android:name="com.bluetoothtestapp.btstartservice" />
            </intent-filter>
        </service>
    </application>
    <uses-sdk android:minSdkVersion="5" android:targetSdkVersion="15" />
    <uses-permission android:name="android.permission.BLUETOOTH" />
    <uses-permission android:name="android.permission.BLUETOOTH_ADMIN" />
</manifest>
