    <string name="tutorial_app_name">Acerca del desbloqueo facial</string>
    <string name="tutorial_header">Mira al teléfono para desbloquearlo.</string>
    <string name="tutorial_info_text">"Ten en cuenta lo siguiente:

 "<li><ul>"La opción de desbloqueo facial es menos segura que un patrón, un PIN o una contraseña.

 "<li>Una persona que tenga unos rasgos parecidos a los tuyos puede desbloquear el teléfono.</li>"

 "<li>Los datos utilizados para identificar tu rostro se almacenan de forma privada en el teléfono.</li>"

 "</ul></li></string>
    <string name="tutorial_continue_label">Configurar</string>
    <string name="intro_header">Para obtener mejores resultados:</string>
    <string name="intro_info_text">" "<li><ul>"Busca un espacio interior con una iluminación adecuada, que no sea demasiado intensa ni demasiado tenue.

 "<li>Sitúa el teléfono a la altura de los ojos.</li>"

 "</ul></li></string>
    <string name="intro_continue_label">Continuar</string>
    <string name="endscreen_header">Ya has terminado.</string>
    <string name="endscreen_top_info_text">Para desbloquear el teléfono, solo tienes que mirarlo.</string>
    <string name="endscreen_bottom_info_text">Sugerencia: para mejorar el reconocimiento facial, captura tu cara de formas diferentes.</string>
    <string name="endscreen_continue_label">Aceptar</string>
    <string name="addtosetup_app_name">Mejorar el reconocimiento facial</string>
    <string name="addtosetup_header">Captura tu cara de formas diferentes.</string>
    <string name="addtosetup_info_text">"Por ejemplo:

 "<li><ul>"con distinta iluminación,

 "<li>con gafas de vista o sin ellas,</li>"

 "<li>con barba y afeitado.</li>"

 "</ul></li>"
Sitúa el teléfono a la altura de los ojos."</string>
