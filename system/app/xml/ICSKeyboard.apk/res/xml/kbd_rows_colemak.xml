<?xml version="1.0" encoding="utf-8"?>
<merge
  xmlns:latin="http://schemas.android.com/apk/res/inputmethod.latin.ported">
    <include latin:keyboardLayout="@xml/kbd_key_styles" />
    <Row latin:keyWidth="10.000002%p">
        <Key latin:moreKeys="1" latin:keyLabel="q" latin:keyHintLabel="1" />
        <Key latin:moreKeys="2" latin:keyLabel="w" latin:keyHintLabel="2" />
        <Key latin:moreKeys="3" latin:keyLabel="f" latin:keyHintLabel="3" />
        <Key latin:moreKeys="4" latin:keyLabel="p" latin:keyHintLabel="4" />
        <Key latin:moreKeys="5" latin:keyLabel="g" latin:keyHintLabel="5" />
        <Key latin:moreKeys="6" latin:keyLabel="j" latin:keyHintLabel="6" />
        <Key latin:moreKeys="7" latin:keyLabel="l" latin:keyHintLabel="7" />
        <Key latin:moreKeys="8" latin:keyLabel="u" latin:keyHintLabel="8" />
        <Key latin:moreKeys="9" latin:keyLabel="y" latin:keyHintLabel="9" />
        <Key latin:moreKeys="0" latin:keyLabel="'" latin:keyHintLabel="0" latin:keyWidth="fillRight" />
    </Row>
    <Row latin:keyWidth="10.000002%p">
        <Key latin:keyLabel="a" />
        <Key latin:keyLabel="r" />
        <Key latin:keyLabel="s" />
        <Key latin:keyLabel="t" />
        <Key latin:keyLabel="d" />
        <Key latin:keyLabel="h" />
        <Key latin:keyLabel="n" />
        <Key latin:keyLabel="e" />
        <Key latin:keyLabel="i" />
        <Key latin:keyLabel="o" />
    </Row>
    <Row latin:keyWidth="10.000002%p">
        <Key latin:keyStyle="shiftKeyStyle" latin:visualInsetsRight="0.99999905%p" latin:keyWidth="14.999998%p" />
        <Key latin:keyLabel="z" />
        <Key latin:keyLabel="x" />
        <Key latin:keyLabel="c" />
        <Key latin:keyLabel="v" />
        <Key latin:keyLabel="b" />
        <Key latin:keyLabel="k" />
        <Key latin:keyLabel="m" />
        <Key latin:keyStyle="deleteKeyStyle" latin:visualInsetsLeft="0.99999905%p" latin:keyWidth="fillRight" />
    </Row>
    <include latin:keyboardLayout="@xml/kbd_qwerty_row4" />
</merge>
