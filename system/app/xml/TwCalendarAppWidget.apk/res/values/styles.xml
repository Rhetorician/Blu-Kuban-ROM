<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Alert" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@null</item>
    </style>
    <style name="MonthView_WeekLabel">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/month_week_w</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_weight">1.0</item>
    </style>
    <style name="MonthView_WeekNameLabel" parent="@style/MonthView_WeekLabel">
        <item name="android:textColor">@color/month_week_name</item>
    </style>
    <style name="TextAppearance" parent="@android:style/TextAppearance" />
    <style name="TextAppearance.MonthView_SunDayLabel" parent="@style/TextAppearance">
        <item name="android:textColor">@color/daynames_sun_text_color</item>
    </style>
    <style name="TextAppearance.MonthView_SaturDayLabel" parent="@style/TextAppearance">
        <item name="android:textColor">@color/daynames_sat_text_color</item>
    </style>
    <style name="TextAppearance.AgendaView_ValueLabel" parent="@style/TextAppearance">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">@android:color/black</item>
    </style>
    <style name="TextAppearance.Alert_Title" parent="@style/TextAppearance">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textColor">@android:color/white</item>
    </style>
    <style name="TextAppearance.Alert_Label" parent="@style/TextAppearance">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@android:color/white</item>
    </style>
    <style name="TextAppearance.Alert_Value" parent="@style/TextAppearance">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">@android:color/white</item>
    </style>
    <style name="CalendarTheme" parent="@android:style/Theme.NoTitleBar">
        <item name="android:windowContentOverlay">@null</item>
    </style>
    <style name="CalendarThemeTitle" parent="@android:style/Theme">
        <item name="android:windowContentOverlay">@null</item>
    </style>
</resources>
