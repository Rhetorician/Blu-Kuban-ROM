            </intent-filter>
            <meta-data android:name="android.accounts.AccountAuthenticator" android:resource="@xml/authenticator_linkedin" />
        </service>
        <provider android:name=".SnsAccountLiContentProvider" android:authorities="com.sec.android.app.provider.snsaccountli" />
        <receiver android:name=".SnsAccountLiBroadcastReceiver">
