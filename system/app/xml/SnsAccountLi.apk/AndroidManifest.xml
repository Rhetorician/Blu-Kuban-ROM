        <activity android:label="SNS AccountLi" android:name=".activity.SnsAccountLiAuthenticatorActivity" android:configChanges="keyboardHidden|orientation|screenSize" android:windowSoftInputMode="stateAlwaysVisible">
            <intent-filter>
                <action android:name="com.sec.android.app.snsaccountli.AuthenticatorActivity" />
