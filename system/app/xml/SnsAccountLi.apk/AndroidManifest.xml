        <activity android:label="" android:name=".activity.SnsAccountLiInformationActivity" android:configChanges="keyboardHidden|orientation|screenSize">
            <intent-filter>
                <action android:name="com.sec.android.app.snsaccountli.SetupAccount" />
                <action android:name="com.sec.android.app.snsdisclaimeragree.snsacountli" />
                <category android:name="android.intent.category.DEFAULT" />
