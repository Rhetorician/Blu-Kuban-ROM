<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="layout_match_parent">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
    <style name="layout_wrap_content">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="layout_equalizer_bar">
        <item name="android:background">@drawable/voice_recorder_equalizer_00</item>
        <item name="android:layout_width">30.0px</item>
        <item name="android:layout_height">59.0px</item>
        <item name="android:layout_marginRight">2.0px</item>
    </style>
    <style name="layout_equalizer_bar_step">
        <item name="android:visibility">invisible</item>
        <item name="android:layout_width">30.0px</item>
        <item name="android:layout_height">3.0px</item>
        <item name="android:layout_marginBottom">1.0px</item>
    </style>
    <style name="layout_time_digit">
        <item name="android:visibility">invisible</item>
        <item name="android:layout_width">37.0px</item>
        <item name="android:layout_height">52.0px</item>
    </style>
    <style name="text_main">
        <item name="android:textSize">24.0px</item>
        <item name="android:textColor">#ff898989</item>
        <item name="android:shadowColor">#ff000000</item>
        <item name="android:shadowRadius">1.0</item>
    </style>
    <style name="button_text" parent="@style/layout_wrap_content">
        <item name="android:textSize">24.0px</item>
        <item name="android:textColor">#ffffffff</item>
        <item name="android:layout_marginTop">38.0px</item>
    </style>
    <style name="tw_list_item_text" parent="@style/layout_match_parent">
        <item name="android:ellipsize">marquee</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="tw_list_item_text.upper_label" parent="@style/tw_list_item_text">
        <item name="android:textSize">33.0px</item>
        <item name="android:textColor">#ffffffff</item>
    </style>
    <style name="tw_list_item_text.bottom_label" parent="@style/tw_list_item_text">
        <item name="android:textSize">21.0px</item>
    </style>
    <style name="dialog_checked_text_view">
        <item name="android:textAppearance">?android:textAppearanceLarge</item>
        <item name="android:textColor">?android:textColorPrimaryInverseDisableOnly</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:paddingLeft">23.0px</item>
        <item name="android:paddingRight">12.0px</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">?android:listPreferredItemHeight</item>
        <item name="android:checkMark">?android:listChoiceIndicatorSingle</item>
    </style>
</resources>
