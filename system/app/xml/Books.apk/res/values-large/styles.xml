        <item name="android:layout_height">@dimen/plus_one_signup_window_height</item>
    </style>
    <style name="Theme.PlusOneSignupDay" parent="@android:style/Theme.Holo.Light.Dialog">
        <item name="android:windowNoTitle">true</item>
