<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="MH.Dialog.NoTitle" parent="@android:style/Theme.Dialog">
        <item name="android:windowBackground">@color/white_1</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
    </style>
    <style name="MH.Holo.Dialog.NoTitle" parent="@android:style/Theme.Holo.Dialog">
        <item name="android:windowBackground">@color/white_1</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
    </style>
    <style name="MH.Holo.Dialog" parent="@android:style/Theme.Holo.Dialog" />
    <style name="MH.Main.Holo" parent="@android:style/Theme.Holo">
        <item name="android:autoCompleteTextViewStyle">@style/SearchViewTextCursor</item>
    </style>
    <style name="SearchViewTextCursor" parent="@android:style/Widget.Holo.AutoCompleteTextView">
        <item name="android:textCursorDrawable">@drawable/search_textfield_cursor</item>
    </style>
</resources>
