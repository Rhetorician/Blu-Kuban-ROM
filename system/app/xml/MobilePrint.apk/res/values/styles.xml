        <item name="android:textColor">#ffffffff</item>
        <item name="android:shadowColor">#ff000000</item>
        <item name="android:shadowDx">-1.0</item>
        <item name="android:shadowDy">-1.0</item>
        <item name="android:shadowRadius">0.3</item>
    </style>
    <style name="textAppearance_black_fg_white_neon">
        <item name="android:textColor">#ff000000</item>
        <item name="android:shadowColor">#ffffffff</item>
        <item name="android:shadowDx">0.0</item>
        <item name="android:shadowDy">0.0</item>
        <item name="android:shadowRadius">2.0</item>
    </style>
    <style name="textAppearance_white_fg_black_solid">
        <item name="android:textColor">#ffffffff</item>
        <item name="android:shadowColor">#ff000000</item>
        <item name="android:shadowDx">0.0</item>
        <item name="android:shadowDy">-1.0</item>
        <item name="android:shadowRadius">1.0</item>
    </style>
    <style name="textAppearance_black_fg_white_solid">
        <item name="android:textColor">#ff000000</item>
        <item name="android:shadowColor">#ffffffff</item>
        <item name="android:shadowDx">0.0</item>
        <item name="android:shadowDy">1.0</item>
        <item name="android:shadowRadius">1.0</item>
    </style>
    <style name="textAppearance_cyan_fg">
        <item name="android:textColor">#ff5397c7</item>
    </style>
    <style name="MobilePrintTheme" parent="@android:style/Theme.DeviceDefault">
        <item name="android:windowShowWallpaper">false</item>
    </style>
    <style name="MobilePrintTheme.Dialog" parent="@style/MobilePrintTheme">
        <item name="android:windowBackground">@android:color/transparent</item>
