            </intent-filter>
        </service>
        <service android:name="com.sec.print.mobileprint.dm.DeviceManagerService">
            <intent-filter>
                <action android:name="com.sec.android.app.mobileprint.SAPS.DeviceManagerService" />
            </intent-filter>
        </service>
    </application>
