        </activity>
        <activity android:label="@string/print_settings" android:name=".MPrintSettingActivity" android:configChanges="mcc|mnc|keyboard|keyboardHidden|orientation|screenSize" />
        <activity android:label="@string/print_settings" android:name=".MPrintSettingSplitActivity" android:configChanges="mcc|mnc|keyboard|keyboardHidden|orientation|screenSize" />
        <activity android:theme="@style/MobilePrintTheme.Dialog" android:name=".MPrintProgressActivity" android:configChanges="mcc|mnc|keyboard|keyboardHidden|orientation|screenSize" />
